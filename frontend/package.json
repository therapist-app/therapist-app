{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run watch:translations\"",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint \"**/src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"**/src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --check \"**/src/**/*.{ts,tsx,js,jsx,css,scss,json}\"",
    "format:fix": "prettier --write \"**/src/**/*.{ts,tsx,js,jsx,css,scss,json}\"",
    "check-all": "npm run lint && npm run format",
    "fix-all": "npm run lint:fix && npm run format:fix",
    "prepare": "cd .. && husky frontend/.husky",
    "generate:resources": "i18next-resources-for-ts interface -i ./public/locales -o ./src/@types/resources.d.ts",
    "watch:translations": "cross-env nodemon --watch public/locales/**/*.json --exec 'npm run generate:resources'"
  },
  "lint-staged": {
    "**/src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/roboto": "^5.1.1",
    "@mui/icons-material": "^6.4.5",
    "@mui/material": "^6.4.5",
    "@mui/x-date-pickers": "^7.28.0",
    "@reduxjs/toolkit": "^2.6.0",
    "axios": "^1.7.9",
    "date-fns": "^2.30.0",
    "i18next": "^24.2.2",
    "i18next-browser-languagedetector": "^8.0.4",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-i18next": "^15.4.1",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^7.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "i18next-resources-for-ts": "^1.5.0",
    "lint-staged": "^15.4.3",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.2",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.25.0",
    "typescript-json-schema": "^0.65.1",
    "vite": "^6.1.0",
    "vite-node": "^3.0.7",
    "vite-plugin-eslint": "^1.8.1"
  }
}
